@box-shadow: 1px 3px 10px #999;
@color-border-panel: rgb(204, 204, 204);
@color-border-table: #757575;

#ludwig_autocomplete_list{
  list-style: none;
  margin-left: 5px;
  padding-left: 0;
  background: #fff;
  //box-shadow: @box-shadow;
  border: 1px solid @color-border-table;
}

#ludwig_autocomplete_list li{
  border-bottom: 1px solid @color-border-panel;
  padding-left: 2px;
}

#ludwig_autocomplete_list li:hover{
  cursor: pointer;
}

#AL_active{
  background: #429E42;
  color: #ffffff;
  border-bottom: 1px solid #FFF
}

#ludwig_autocomplete_list :last-child{
  border-bottom: 0;
}
.AL_animation_newRecord_in{
  transition:all 3s ease;
  background: rgba(16, 156, 27, 0.31);
}
.AL_animation_newRecord_out{
  transition:all 3s ease;
  background: rgba(0, 0, 0, 0);
}